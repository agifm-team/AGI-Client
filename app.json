{
  "expo": {
    "name": "Pony House",
    "slug": "pony-house-matrix-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./build/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./build/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#282c34"
