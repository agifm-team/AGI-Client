{
  "name": "pixxels-matrix",
  "version": "1.3.38",
  "short_name": "Pixxels",
  "description": "Pixxels: Collaborative AI",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "setup": "yarn && node -r esm ./build/index.js ",
    "start": "node -r esm ./build/start/vite.js && vite dev",
    "build": "node -r esm ./build/start/vite.js && vite build",
    "build:8gb": "node -r esm ./build/start/vite.js && cross-env NODE_OPTIONS=--max-old-space-size=8192 vite build",
    "electron:start": "node -r esm ./build/start/vite.js && cross-env ELECTRON_MODE=true vite dev",
    "electron:build": "node -r esm ./build/start/vite.js && cross-env ELECTRON_MODE=true vite build && electron-builder",
    "expo:start": "node -r esm ./build/start/expo.js && cross-env EXPO_MODE=true expo start",
    "android": "node -r esm ./build/start/expo.js && cross-env EXPO_MODE=true expo start --android",
    "ios": "node -r esm ./build/start/expo.js && cross-env EXPO_MODE=true expo start --ios",
    "web": "node -r esm ./build/start/expo.js && cross-env EXPO_MODE=true expo start --web",
    "lint": "yarn check:eslint && yarn check:prettier",
    "check:eslint": "eslint src/* && eslint electron/* && eslint mods/* --no-error-on-unmatched-pattern",
    "check:prettier": "prettier --check src/* && prettier --check electron/* && prettier --check mods/*",
    "fix:prettier": "prettier --write src/* && prettier --write electron/* && prettier --write mods/*",
    "fix:src:prettier": "prettier --write src/*",
    "fix:electron:prettier": "prettier --write electron/*",
    "fix:mods:prettier": "prettier --write mods/*",
    "public:check:eslint": "eslint public/*",
    "public:check:prettier": "prettier --check public/*",
    "public:fix:prettier": "prettier --write public/*",
    "pony-merge": "git fetch upstream && git merge upstream/dev"
  },
  "keywords": [
    "react",
    "javascript",
    "website",
    "matrix",
    "matrix-client",
    "Pixxels",
    "Pixxels-App"
  ],
  "author": {
    "name": "seshu.bonam",
    "email": "seshu.bonam@gmail.com",
    "url": "https://github.com/seshubonam"
  },
